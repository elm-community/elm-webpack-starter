    "elm-webpack-loader": "2.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.9.0",
    "node-sass": "3.4.2",
    "postcss-loader": "0.8.1",
    "rimraf": "2.5.2",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "webpack": "1.12.13",
    "webpack-dev-server": "1.14.1",
    "webpack-merge": "0.7.3"
  }
}
